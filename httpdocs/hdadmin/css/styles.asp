}
td.border {
border-style: solid;
border-color: #CCCCCC;
border-width: 1px;
padding:5px;
}
td.titleborder {
border-style: solid;
border-color: #CCCCCC;
border-width: 1px;
padding:5px;
background-image: url('<%=hdAdminPath%>images/titlebckg.jpg');
}

a { color:#0000CC; font-size: 12px; font-family : Tahoma, Arial, Helvetica, sans-serif;}
a:link { color:#0000CC; text-decoration:none; font-weight:normal }
a:visited { color:#0000CC; text-decoration:none; font-weight:normal }
a:hover { color:#FF9900; text-decoration:none; font-weight:normal }
a:active { text-decoration: none; }

a.nav { color:#0000CC; font-size: 14px; font-family : Tahoma, Arial, Helvetica, sans-serif;}
a.nav:link { color:#0000CC; text-decoration:none; font-weight:normal }
a.nav:visited { color:#FF9900; text-decoration:none; font-weight:normal }
a.nav:hover { color:#000000; text-decoration:none; font-weight:normal }
a.nav:active { text-decoration: none; }

a.tnav { color:#FFFFFF; font-size: 14px; font-family : Tahoma, Arial, Helvetica, sans-serif;}
a.tnav:link { color:#FFFFFF; text-decoration:none; font-weight:normal }
a.tnav:visited { color:#FFFFFF; text-decoration:none; font-weight:normal }
a.tnav:hover { color:#FF9900; text-decoration:none; font-weight:normal }
a.tnav:active { text-decoration: none; }

